<p id="notice"><%= notice %></p>

<h1>Pre Registrants</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pre_registrants.each do |pre_registrant| %>
      <tr>
        <td><%= pre_registrant.name %></td>
        <td><%= pre_registrant.email %></td>
        <td><%= link_to 'Show', pre_registrant %></td>
        <td><%= link_to 'Edit', edit_pre_registrant_path(pre_registrant) %></td>
        <td><%= link_to 'Destroy', pre_registrant, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pre Registrant', new_pre_registrant_path %>
